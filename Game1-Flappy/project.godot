; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Game 1-flappy"
run/main_scene="res://BaseScene.tscn"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1600
window/size/viewport_height=720

[dotnet]

project/assembly_name="Game 1-flappy"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg")

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[layer_names]

2d_render/layer_1="Background"
2d_render/layer_2="Foreground"
2d_render/layer_3="Overlay"
2d_physics/layer_1="Player"
2d_physics/layer_2="Obstacles"
2d_physics/layer_3="ScoreGate"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
